                   },
                   "template_text3" : { 
                       en : "App name",
                       es : "Nombre App",
                       ca : "Nom App"
                   },
                   "clean_title" : { 
                       en : "Clean generated SMS",
                       es : "Borra SMS generados",
                       ca : "Esborra SMS generats"
                   },
                   "clean_text" : { 
                       en : "With remove SMS option you can easily clean your generated messages SMS inbox",
                       es : "Con la opción de eliminación de SMS generados podrás limpiar fácilment la bandeja de entrada de SMS",
                       ca : "Amb l'opció d'eliminació de SMS generatos podràs netejar fàcilment la safata d'entrada d'SMS"
                   },
                   
                   /*FAQ*/
                   
                   "question1" : {
                   	 	 en : "Can I still receive SMS normally?",
                       es : "¿Puedo seguir recibiendo SMS normalmente?",
                       ca : "Pot continuar rebent SMS normalment?"
                   },
                   "answer1" : {
                   	 	 en : "Yes, you will receive SMS normally but you can't write new SMS from NotifiCast. You'll need your habitual app to write SMS",
                       es : "Sí, recibirás SMS con normalidad, pero no se puede escribir un nuevo SMS amb NotifiCast. Necesitarás tu aplicación habitual para escribir SMS",
                       ca : "Sí, rebràs SMS amb normalitat, però no es pot escriure un nou SMS amb NotifiCast. Fes servir la teva aplicació habitual per escriure SMS"
                   },
                   
                   "question2" : {
                   	 	 en : "Is there any extra cost caused by the SMS generation?",
                       es : "¿Hay algún cargo extra por generar SMS?",
                       ca : "Hi ha cap càrrec extra per generar SMS?"
                   },
                   "answer2" : {
                   	 	 en : "No, each SMS generated by NotifiCast is completely free. Don't worry about extra costs!",
                       es : "No, cada SMS generado por NotifiCast es completamente gratuito. No te preocupes por costes extra!!",
                       ca : "No, cada SMS generat per NotifiCast és completament gratuït. No te preocupis per despese extra!!"
                   },
                   
                   "question3" : {
                   	 	 en : "How do I know if my car is compatible with NotifiCast?",
                       es : "¿Cómo saber si mi coche es compatible con NotifiCast?",
                       ca : "Com saber si el meu cotxe és compatible amb NotifiCast?"
                   },
                   "answer3" : {
                   	 	 en : "If you can read your SMS from the car NotifiCast is compatible. I've tested in a Mazda 3 of 2015 but NotifiCast doesn't depend of any brand only needs SMS sync via bluetooth",
